## install-euca2ools-deps
#

host_packages+=(
    'gzip' 'openssl' 'python-dev' 'python-lxml' 'python-requests'
    'python-setuptools' 'python-six'
)

# The python-support package no longer exists in Stretch.
if [ "${ID}" = "debian" ] && [ -n "${VERSION_ID}" ] && [ ${VERSION_ID} -le 8 ]
then
    host_packages+=('python-support')
fi
